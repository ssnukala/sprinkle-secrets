{
    "name": "ssnukala/sprinkle-secrets",
    "type": "userfrosting-sprinkle",
    "description": "Secrets Module for UserFrosting 5.0.",
    "keywords": [
        "php user management",
        "userfrosting",
        "sprinkle",
        "secrets"
    ],
    "homepage": "https://github.com/userfrosting/UserFrosting",
    "license": "MIT",
    "authors": [
        {
            "name": "Srinivas Nukala",
            "homepage": "https://srinivasnukala.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "birke/rememberme": "^2.0",
        "nikic/php-parser": "^4.2.2",
        "userfrosting/framework": "^5.0",
        "userfrosting/sprinkle-core": "^5.0",
        "aws/aws-sdk-php": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "slam/phpstan-extensions": "^6.0",
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.2",
        "fakerphp/faker": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "UserFrosting\\Sprinkle\\Secrets\\": "app/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserFrosting\\Sprinkle\\Secrets\\Tests\\": "app/tests/"
        }
    }
}